{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "warn",
      "default-case": 0,
      "no-loop-func": "off",
      "no-cond-assign": "off",
      "no-param-reassign": "off",
      "no-await-in-loop": 0,
      "object-curly-newline": 0,
      "no-shadow": 0
    }
}
